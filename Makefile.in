#### Start of (auto)configure specific section. ####

Makefile: $(srcdir)/Makefile.in $(top_builddir)/config.status
	cd $(top_builddir) \
	  && CONFIG_FILES=$@ CONFIG_HEADERS= $(SHELL) ./config.status

config.status: $(srcdir)/configure
	$(SHELL) ./config.status --recheck

$(srcdir)/configure: $(srcdir)/configure.in $(top_srcdir)/aclocal.m4
	@echo "Warning: $(srcdir)/configure is out of date"
	cd $(srcdir) && rm -f configure && autoconf

config.h: stamp-h
	@if test ! -f $@; \
	then \
	  rm -f stamp-h; \
	  $(MAKE) stamp.h; \
	else \
	  : ; \
	fi

stamp-h: $(srcdir)/config.hin $(top_builddir)/config.status
	@cd $(top_builddir) \
	  && CONFIG_FILES= CONFIG_HEADERS="config.h:config.hin" \
	     $(SHELL) ./config.status
	@echo "timestamp" > stamp-h 2> /dev/null

$(srcdir)/config.hin: $(srcdir)/stamp-h.in
	@if test ! -f $@; \
	then \
	  rm -f $(srcdir)/stamp-h.in; \
	  $(MAKE) $(srcdir)/stamp-h.in; \
	else \
	  : ; \
	fi

$(srcdir)/stamp-h.in: $(top_srcdir)/configure.in $(top_srcdir)/aclocal.m4 acconfig.h
	cd $(top_srcdir) && autoheader
	@echo "timestamp" > $(srcdir)/stamp-h.in 2> /dev/null

#### End of (auto)configure specific section. ####



#### Start of clean section. ####

archiveclean:
	-rm -f *.gz \
	       *.z \
	       *.Z \
	       *.zip \
	       *.zoo \
	       *.lzh \
	       *.shar*

distclean-local: archiveclean
	-rm -f Makefile \
	       config.status \
	       config.cache \
	       config.log \
	       config.h \
	       stamp-h \
	       install-info

realclean-local: distclean-local
	-rm -f ID id TAGS tags

maintainer-clean-local: realclean-local
	@echo "This command is intended only for maintainers to use;"
	@echo "rebuilding the deleted files may require special tools."

distclean: distclean-recursive distclean-local

realclean: realclean-recursive distclean-local

maintainer-clean: maintainer-clean-recursive maintainer-clean-local

#### End of clean section. ####



#### Start of install section. ####

install: install-recursive
	@echo "All installs done"

#### End of install section. ####



#### Start of basic targets section. ####

install-info: $(srcdir)/install-inf.in $(top_builddir)/config.status
	@cd $(top_builddir) \
	  && CONFIG_FILES=install-inf CONFIG_HEADERS= $(SHELL) ./config.status
	-if test -s install-inf; \
	then \
	  mv install-inf $@; \
	  chmod u+x $@; \
	else \
	  : ; \
	fi

#
# `perl' preferred!
#
$(srcdir)/$(MANIFEST): Makefile $(MANIFEST)-recursive $(DISTFILES)
	@if test ! -s "$@"; \
	then \
	  echo "" > $@; \
	  if test -n "$(AWK)" || test -n "$(PERL)"; \
	  then \
	    echo "Creating \`$@', please wait..."; \
	    distname=$(PACKAGE)-$(VERSION); \
	    rm -rf $$distname; \
	    mkdir $$distname; \
	    chmod $(DISTBINPERMS) $$distname; \
	    for dir in $(DISTDIRS); \
	    do \
	      mkdir $$distname/$$dir; \
	      chmod $(DISTBINPERMS) $$distname/$$dir; \
	    done; \
	    for file in $(DISTFILES) $(MANIFEST) $(CREATEFILES); \
	    do \
	      ln $(srcdir)/$$file $$distname/$$file 2>/dev/null || \
	      { \
	        echo "Cannot link, copying \`$(srcdir)/$$file'"; \
	        if test -n "$(CP)"; \
	        then \
	          $(CP) $(srcdir)/$$file $$distname/$$file; \
	        else \
	          cat $(srcdir)/$$file > $$distname/$$file; \
	        fi; \
	      }; \
	    done; \
	    if test -n "$(PERL)"; \
	    then \
	      ls -lR $$distname | \
	      grep '^[^d]' | \
	      $(PERL) -e '$$[=1;$$d="/";$$b=".".$$d;$$p=$$b;$$x=0;$$o=0;' \
	              -e 'while(<>){chop;@Fld=split(" ",$$_,9999);' \
	              -e 'if(($$.-$$FNRbase)>1){if($$#Fld==1){$$l=length($$_);' \
	              -e 'for($$i=1;$$i<=$$l;$$i++){if(substr($$_,$$i,1)eq$$d)' \
	              -e '{$$p=$$b.substr($$_,$$i+1,$$l-$$i-1).$$d;$$i=$$l;$$x=1;}}}' \
	              -e 'else{if($$x==1){$$x=0;}else{$$f=$$p.$$Fld[$$#Fld];$$o=1;}}}' \
	              -e 'if($$o==1){printf"%s\n",$$f;$$o=0;}}' > $@; \
	    else \
	      ls -lR $$distname | \
	      grep '^[^d]' | \
	      $(AWK) 'BEGIN{d="/";b="."d;p=b;x=0;o=0} \
	              {if(FNR>1){if(NF==1){l=length($$0); \
	              for(i=1;i<=l;i++){if(substr($$0,i,1)==d) \
	              {p=b""substr($$0,i+1,l-i-1)""d;i=l;x=1;}}} \
	              else{if(x==1){x=0;}else{f=p""$$NF;o=1;}}} \
	              if(o==1){printf"%s\n",f;o=0;}}' > $@; \
	    fi; \
	    rm -rf $$distname; \
	  else \
	    echo "Unable to create \`$@', missing \`awk' and \`perl'"; \
	  fi; \
	else \
	  echo "Nothing to be done for \`$@'."; \
	fi

