#### Start of system configuration section. ####

PACKAGE = @PACKAGE@
VERSION = @VERSION@

srcdir = @srcdir@
top_srcdir = @top_srcdir@
VPATH = @srcdir@

top_builddir = .

@SET_MAKE@

doclang = @doclang@

INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA = @INSTALL_DATA@

SHAR = @SHAR@
ZIP = @ZIP@
ZOO = @ZOO@
LHA = @LHA@
TAR = @TAR@

Gzip = @GZIP@
COMPRESS = @COMPRESS@

AWK = @AWK@
PERL = @PERL@

CP = @CP@

#### End of system configuration section. ####



#### Start of package specific make variable definition section. ####

SHELL = /bin/sh

# Extension (not including `.') for the installed manual page file names.
manext = 1

#
# Compressor/archiver flags.
#
SHARFLAGS = -MF
ZIPFLAGS = -ur9
ZOOFLAGS = ahI
LHAFLAGS = u
TARFLAGS = -cvf
GZIPFLAGS = -c9
COMPRESSFLAGS = -c

#
# File owner permissions used for all distribution targets.
#
DISTBINPERMS = 0777
DISTSRCPERMS = 0666
#
# File owner permissions used for the installdata and installdates target
# and for resetting the permissions after running a distribution target.
#
BINPERMS = `echo $(INSTALL_PROGRAM) | sed -e 's,^.*/,,' -e 's/[^0-9]//g'`
SRCPERMS = `echo $(INSTALL_DATA) | sed -e 's,^.*/,,' -e 's/[^0-9]//g'`
#
# File owner permissions used for installdata and installdates target
# and for resetting the permissions after running a distribution target
# if the configure script doesn't set $(BINPERMS) or $(SRCPERMS).
#
BINPERMS2 = 0755
SRCPERMS2 = 0644

