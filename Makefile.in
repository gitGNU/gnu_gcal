$(MANIFEST)-recursive: Makefile config.h
	@target=`echo "$@" | sed -e 's/-recursive//'`; \
	if test ! -f "$$target"; \
	then \
	  for subdir in $(SUBDIRS); \
	  do \
	    echo "Making $$target in $$subdir"; \
	    (cd $$subdir && $(MAKE) $$target) \
	     || case "$(MFLAGS)" in *k*) fail=yes;; *) exit 1;; esac; \
	  done && test -z "$$fail"; \
	else \
	  : ; \
	fi

