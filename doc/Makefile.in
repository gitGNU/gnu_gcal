install: installdirs installman installinfo
	@echo "All installs in $(subdir) done"

installdirs: $(top_srcdir)/mkinstalldirs
	$(NORMAL_INSTALL)
	$(SHELL) $(top_srcdir)/mkinstalldirs $(ALLDIRS)

#
# Converts all `*.$(manext)' files by means of `perl' (preferred) or `awk'
# in order to put real ISO-8859-1 characters into the resulting file.
#
installman: installdirs $(docmanFILES)
	$(PRE_INSTALL)
	@for file in $(manFILES); \
	do \
	  rm -f $(mandir)/$$file.$(manext); \
	done
	$(NORMAL_INSTALL)
	@case "$(doclang)" in \
	  $(de) | $(en)) \
	    ;; \
	  *) \
	    echo "$@: error: LANGUAGE=$(doclang) is unsupported"; \
	    exit 1;; \
	esac; \
	for file in $(manFILES); \
	do \
	  if test -n "$(PERL)"; \
	  then \
	    echo "$(PERL) converts all accent characters of \`$(srcdir)/$(doclang)/$(docmanDIR)/$$file.$(manext)' ..."; \
	    $(PERL) $(srcdir)/2accent.pl $(srcdir)/$(doclang)/$(docmanDIR)/$$file.$(manext) \
	      > $$file.$(manext); \
	  else \
	    if test -n "$(AWK)"; \
	    then \
	      echo "$(AWK) converts all accent characters of \`$(srcdir)/$(doclang)/$(docmanDIR)/$$file.$(manext)' ..."; \
	      $(AWK) -f $(srcdir)/2accent.awk $(srcdir)/$(doclang)/$(docmanDIR)/$$file.$(manext) \
	        > $$file.$(manext); \
	    else \
	      echo "Accent characters not converted, missing \`perl' / \`awk'"; \
	      if test -n "$(CP)"; \
	      then \
	        $(CP) $(srcdir)/$(doclang)/$(docmanDIR)/$$file.$(manext) $$file.$(manext); \
	      else \
	        cat $(srcdir)/$(doclang)/$(docmanDIR)/$$file.$(manext) > $$file.$(manext); \
	      fi; \
	    fi; \
	  fi; \
	  echo "Installing \`$$file.$(manext)' for LANGUAGE=$(doclang) users as \`$(mandir)/$$file.$(manext)'"; \
	  $(INSTALL_DATA) $$file.$(manext) $(mandir)/$$file.$(manext); \
	  rm -f $$file.$(manext); \
	done
	@echo "LANGUAGE=$(doclang) manual pages installed"

#
# Install native language specific `$(PACKAGE).info*' files into $(infodir).
#
installinfo: installdirs $(docINFOFILES)
	$(PRE_INSTALL)
	@rm -f $(infodir)/$(PACKAGE).info*
	$(NORMAL_INSTALL)
	@case "$(doclang)" in \
	  $(de) | $(en)) \
	    ;; \
	  *) \
	    echo "$@: error: LANGUAGE=$(doclang) is unsupported"; \
	    exit 1;; \
	esac
	@oldwd=`pwd`; \
	cd $(srcdir)/$(doclang); \
	instfiles=`echo $(PACKAGE).info*`; \
	cd $$oldwd; \
	for file in $$instfiles; \
	do \
	  echo "Installing \`$(srcdir)/$(doclang)/$$file' for LANGUAGE=$(doclang) users as \`$(infodir)/$$file'"; \
	  $(INSTALL_DATA) $(srcdir)/$(doclang)/$$file $(infodir)/$$file; \
	done
	$(POST_INSTALL)
	-if $(SHELL) -c 'install-info --version' > /dev/null 2>&1; \
	then \
	  install-info --info-dir=$(infodir) $(srcdir)/$(doclang)/$(PACKAGE).info; \
	else \
	  if $(SHELL) -c '$(top_srcdir)/install-info --version' > /dev/null 2>&1; \
	  then \
	    $(SHELL) $(top_srcdir)/install-info --info-dir=$(infodir) $(srcdir)/$(doclang)/$(PACKAGE).info; \
	  else \
	    : ; \
	  fi; \
	fi
	@echo "LANGUAGE=$(doclang) Info files installed"

#
# Uninstall native language specific `$(PACKAGE).info*' files from $(infodir)
# and the native language specific `*.$(manext)' files from $(mandir).
#
uninstall:
	$(PRE_UNINSTALL)
	-if $(SHELL) -c 'install-info --version' > /dev/null 2>&1; \
	then \
	  install-info --info-dir=$(infodir) --remove $(PACKAGE).info; \
	else \
	  : ; \
	fi
	$(NORMAL_UNINSTALL)
	-for file in $(manFILES); \
	do \
	  rm -f $(mandir)/$$file.$(manext); \
	done
	-rm -f $(infodir)/$(PACKAGE).info*
	@echo "Documentation files of distribution uninstalled"

