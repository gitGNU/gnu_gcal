#
# If not exists, try to create the native language specific HTML documentation
# file for distribution (dist, dist-$(PACKAGE), distzip, distzoo, distlha and
# distshar) from the native language specific `$(PACKAGE).texi' file.
#
# Parallel processing of this target is *NOT* permitted!
#
$(docHTMLFILES): $(doctexiFILES) $(docexamplesTEXIFILES)
	@if test -f "$(lockfile)"; \
	then \
	  echo "Parallel processing of \`$@' target not permitted, $(lockfile) exists"; \
	else \
	  echo "Processing \`$@' ..." > $(lockfile); \
	  for dir in $(ALLDOCS); \
	  do \
	    if test -n "$(MAKEINFO)" && test -n "$(MAKEINFO_VERSION)"; \
	    then \
	      echo "Creating distribution HTML documentation file \`$(srcdir)/$$dir/$(PACKAGE).html'"; \
	      echo "Please wait..."; \
	      case "$$dir" in \
	        $(de) | $(en)) \
	          ;; \
	        *) \
	          echo "$@: error: LANGUAGE=$$dir is unsupported"; \
	          rm -f $(lockfile); \
	          exit 1;; \
	      esac; \
	      if test -n "$(CP)"; \
	      then \
	        $(CP) $(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi $(srcdir)/$$dir/$(PACKAGE).texi; \
	      else \
	        cat $(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi > $(srcdir)/$$dir/$(PACKAGE).texi; \
	      fi; \
	      oldpd=`pwd`; \
	      cd $(srcdir)/$$dir; \
	      LANGUAGE=$$dir $(MAKEINFO) $(MAKEINFOFLAGS_HTML) $(PACKAGE).html $(PACKAGE).texi; \
	      rm -f $(PACKAGE).texi; \
	      cd $$oldpd; \
	      echo "\`$(srcdir)/$$dir/$(PACKAGE).html' created"; \
	    else \
	      if test -z "$(MAKEINFO)"; \
	      then \
	        echo "Unable to create distribution HTML documentation file \`$(srcdir)/$$dir/$(PACKAGE).html', missing \`makeinfo'"; \
	      else \
	        echo "Unable to create distribution HTML documentation file \`$(srcdir)/$$dir/$(PACKAGE).html', \`makeinfo' out of date"; \
	      fi; \
	    fi; \
	  done; \
	  rm -f $(lockfile); \
	fi
html $(PACKAGE).html: $(docHTMLFILES)

#
# If not exists, try to create the native language specific Info documentation
# files for distribution (dist, dist-$(PACKAGE), distzip, distzoo, distlha and
# distshar) from the native language specific `$(PACKAGE).texi' file.
#
# Assume GNU `[g]info' is able to represent 8-bit characters so convert
# all `$(PACKAGE).texi' files by means of `perl' (preferred) or `awk'
# in order to put real ISO-8859-1 characters into the resulting file.
#
# Parallel processing of this target is *NOT* permitted!
#
$(docINFOFILES): $(doctexiFILES) $(docexamplesTEXIFILES)
	@if test -f "$(lockfile)"; \
	then \
	  echo "Parallel processing of \`$@' target not permitted, $(lockfile) exists"; \
	else \
	  echo "Processing \`$@' ..." > $(lockfile); \
	  for dir in $(ALLDOCS); \
	  do \
	    if test -n "$(MAKEINFO)" && test -n "$(MAKEINFO_VERSION)"; \
	    then \
	      echo "Creating distribution Info files \`$(srcdir)/$$dir/$(PACKAGE).info*'"; \
	      echo "Please wait..."; \
	      case "$$dir" in \
	        $(de) | $(en)) \
	          ;; \
	        *) \
	          echo "$@: error: LANGUAGE=$$dir is unsupported"; \
	          rm -f $(lockfile); \
	          exit 1;; \
	      esac; \
	      if test -n "$(PERL)"; \
	      then \
	        echo "$(PERL) converts all accent characters of \`$(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi' ..."; \
	        $(PERL) $(srcdir)/2accent.pl $(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi \
	          > $(srcdir)/$$dir/$(PACKAGE).texi; \
	      else \
	        if test -n "$(AWK)"; \
	        then \
	          echo "$(AWK) converts all accent characters of \`$(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi' ..."; \
	          $(AWK) -f $(srcdir)/2accent.awk $(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi \
	            > $(srcdir)/$$dir/$(PACKAGE).texi; \
	        else \
	          echo "Accent characters not converted, missing \`perl' / \`awk'"; \
	          if test -n "$(CP)"; \
	          then \
	            $(CP) $(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi $(srcdir)/$$dir/$(PACKAGE).texi; \
	          else \
	            cat $(srcdir)/$$dir/$(doctexiDIR)/$(PACKAGE).texi > $(srcdir)/$$dir/$(PACKAGE).texi; \
	          fi; \
	        fi; \
	      fi; \
	      oldwd=`pwd`; \
	      cd $(srcdir)/$$dir; \
	      LANGUAGE=$$dir $(MAKEINFO) $(MAKEINFOFLAGS) $(PACKAGE).info $(PACKAGE).texi; \
	      rm -f $(PACKAGE).texi; \
	      cd $$oldwd; \
	      echo "\`$(srcdir)/$$dir/$(PACKAGE).info*' created"; \
	    else \
	      if test -z "$(MAKEINFO)"; \
	      then \
	        echo "Unable to create distribution Info documentation files \`$(srcdir)/$$dir/$(PACKAGE).info*', missing \`makeinfo'"; \
	      else \
	        echo "Unable to create distribution Info documentation files \`$(srcdir)/$$dir/$(PACKAGE).info*', \`makeinfo' out of date"; \
	      fi; \
	    fi; \
	  done; \
	  rm -f $(lockfile); \
	fi
info $(PACKAGE).info: $(docINFOFILES)

#
# Try to create the DVI documentation file according to selected native
# language.
#
# OBSOLETE: If a NON-ENGLISH native language is selected, convert the
#           `texinfo.tex' file of the package in order to replace some
#           English words found there:
#
#	        $(de)) \
#	          ...
#	          fi; \
#	          cat $(docTEXINFOFILE) | \
#	          sed -e 's/{Chapter}\\fi/{Kapitel}\\fi/' \
#	              -e 's/{See}\\fi/{Siehe}\\fi/' \
#	              -e 's/{see}\\fi/{siehe}\\fi/' \
#	              -e 's/{file}\\fi/{Datei}\\fi/' \
#	              -e 's/{page}\\fi/{Seite}\\fi/' \
#	              -e 's/{Section}\\fi/{Abschnitt}\\fi/' \
#	              -e 's/{section}\\fi/{Abschnitt}\\fi/' \
#	              -e 's/{Table of Contents}\\fi/{Inhaltsverzeichnis}\\fi/' \
#	              -e 's/{Short Contents}\\fi/{Kurzinhalt}\\fi/' \
#	              -e 's/{Appendix}\\fi/{Anhang}\\fi/' > $(doclang)/$(docTEXINFOFILE);; \
#	        $(en)) \
#	          ...
#
# Parallel processing of this target is *NOT* permitted!
#
$(PACKAGE).dvi: $(doctexiFILES) $(docexamplesTEXIFILES) $(docTEXINFOFILE)
	@if test -f "$(lockfile)"; \
	then \
	  echo "Parallel processing of \`$@' target not permitted, $(lockfile) exists"; \
	else \
	  echo "Processing \`$@' ..." > $(lockfile); \
	  if test -n "$(TEXI2DVI)" && test -n "$(TEXI2DVI_VERSION)"; \
	  then \
	    echo "Creating \`$(srcdir)/$(doclang)/$@' file for LANGUAGE=$(doclang) users"; \
	    echo "Please wait..."; \
	    case "$(doclang)" in \
	      $(de) | $(en)) \
	        ;; \
	      *) \
	        echo "$@: error: LANGUAGE=$(doclang) is unsupported"; \
	        rm -f $(lockfile); \
	        exit 1;; \
	    esac; \
	    copy_target=`echo "$(docTEXINFOFILE)" | sed -e 's,.*/,,'`; \
	    if test -n "$(CP)"; \
	    then \
	      $(CP) $(docTEXINFOFILE) $(srcdir)/$(doclang)/$$copy_target; \
	      $(CP) $(srcdir)/$(doclang)/$(doctexiDIR)/$(PACKAGE).texi $(srcdir)/$(doclang)/$(PACKAGE).texi; \
	    else \
	      cat $(docTEXINFOFILE) > $(srcdir)/$(doclang)/$$copy_target; \
	      cat $(srcdir)/$(doclang)/$(doctexiDIR)/$(PACKAGE).texi > $(srcdir)/$(doclang)/$(PACKAGE).texi; \
	    fi; \
	    oldwd=`pwd`; \
	    cd $(srcdir)/$(doclang); \
	    $(TEXI2DVI) $(PACKAGE).texi; \
	    rm -f $$copy_target \
	          $(PACKAGE).texi; \
	    cd $$oldwd; \
	    echo "\`$(srcdir)/$(doclang)/$@' created"; \
	  else \
	    if test -z "$(TEXI2DVI)"; \
	    then \
	      echo "Unable to create \`$(srcdir)/$(doclang)/$@' file, missing \`texi2dvi'"; \
	    else \
	      echo "Unable to create \`$(srcdir)/$(doclang)/$@' file, \`texi2dvi' out of date"; \
	    fi; \
	  fi; \
	  rm -f $(lockfile); \
	fi
dvi: $(PACKAGE).dvi

