@item --atmosphere=@var{Luftdruck}@r{[,}@var{Temperatur}@r{]}
@arindex --atmosphere=@var{Luftdruck}@r{[,}@var{Temperatur}@r{]}
@cindex Basisdaten der Erdatmosph@"are zur Refraktionsberechnung festlegen
@cindex Erdatmosph@"are, Basisdaten zur Refraktionsberechnung festlegen
@cindex Temperatur der Erdatmosph@"are festlegen
@cindex Erdatmosph@"are, Temperatur festlegen
